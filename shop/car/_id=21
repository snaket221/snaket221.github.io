	<script>
		function carousel(direction) {
			var itemtop = document.querySelector(".body .left .overflow");

			if (window.innerHeight > window.innerWidth){
				var number = 80;
			}

			else {
				var number = 40;
			}

			var total = 2 * - number;
			var overflow = document.querySelector(".absolute");
			var currentleft = overflow.style.left;

			if (currentleft == "") {
				overflow.style.left = "0vw";
			}

			else if (direction == "forward" || direction == null) {
				var newtotal = currentleft.replace("vw", "") - number + "vw";
									if (newtotal.replace("vw", "") == total && direction == null) {
					}

					else if (newtotal.replace("vw", "") == total && direction == "forward") {
						overflow.style.left = "0vw";
					}
				
				else {
					overflow.style.left = newtotal;
				}
			}

			else if (direction == "back") {
				var newtotal = Number(currentleft.replace("vw", "")) + number + "vw";
				if (newtotal == "40vw") {
				}

				else {
					overflow.style.left = newtotal;
				}
			}
		}
	</script>
